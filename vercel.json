{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist/cycle-calendar-tool-angular/browser"
            }
        },
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/$1",
            "check": true
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}